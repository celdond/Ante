{
  "include": [
    "src/assets/app"
  ],
  "exclude": [
    ".build",
    "node_modules"
  ],
  "compilerOptions": {
		"noImplicitReturns": true,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"strict": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"module": "ES2015",
		"target": "ES2015",
    "allowJs": true,
    "moduleResolution": "node",
    "rootDir": "src/assets/app",
		"lib": [
			"ES2015",
			"DOM"
		],
  }
}